{
  "compilerOptions": {
     "outDir": "out",
     "target": "es2017",
     "module": "commonjs",
     "lib": [ "es2017.object", "dom", "es6" ],
     "sourceMap": true,
     "noFallthroughCasesInSwitch": true,
     "noImplicitAny": true,
     "strictNullChecks": true,
     "strictPropertyInitialization": true,
     "esModuleInterop": true
  },
  "include": ["src/**/*.ts"]
}
